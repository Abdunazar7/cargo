<h3>Login page</h3>

<form id="login_form">
  <div class="mb-3">
    <label for="email" class="form-label">Email address</label>
    <input
      type="email"
      class="form-control"
      id="email"
      name="email"
      value="john@example.com"
    />

  </div>
  <div class="mb-3">
    <label for="password" class="form-label">Password</label>
    <input
      type="password"
      class="form-control"
      id="password"
      name="password"
      value="123456"
    />
  </div>

  <button type="submit" class="btn btn-primary">Submit</button>
</form>

<script src="./js/script.js"></script>

<script>
    login()
</script>
